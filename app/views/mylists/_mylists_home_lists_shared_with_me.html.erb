
<h4>Lists shared with you...</h4>
<div class="padding_bottom">
  <table class="table table-striped table-bordered">
    <thead>
      <tr class="blue_white_table_header">
        <th>List Name</th>
        <th>Event Date</th>
      </tr>
    </thead>
    <tbody>
      <% @lists_shared_with_me.each do |sharedlist| %>
        <tr>
          <td><a href="/sharedlists/<%= sharedlist.id %>/contents"><%= sharedlist.listname %></a></td>
          <td><%= sharedlist.eventdate.strftime("%B %d, %Y") %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
