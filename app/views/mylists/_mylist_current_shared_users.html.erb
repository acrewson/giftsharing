      <div class="row">
        <div class="panel panel-primary">
          <div class="panel-heading">
            <h4 class="panel-title">This list is shared with:</h4>
          </div>
          <div class="panel-body">
            <form action="/mylists/<%= @selected_list.id %>/list_access/remove">
            <div class="padding_bottom">
              <table>
                <thead></thead>
                <tbody>
                  <% @shared_users.each do |shared_user| %>
                  <tr>
                  <td>
                    <span class="padding_right_20"><input type="checkbox" name="remove<%= shared_user.id %>" value="true"></span>
                    <%= shared_user.firstname + " " + shared_user.lastname %>
                  </td>
                  </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
            <input type="submit" class="btn btn-md btn-danger" value="Remove access">
            </form>
          </div>
        </div>
      </div>
